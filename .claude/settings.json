{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bash .claude/hooks/git-status.sh"
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "time-awareness": {
      "source": {
        "source": "github",
        "repo": "linuxiscool/claude-plugins-public"
      }
    },
    "regen": {
      "source": {
        "source": "github",
        "repo": "gaiaaiagent/regen-ai-claude"
      }
    },
    "planning": {
      "source": {
        "source": "github",
        "repo": "OthmanAdi/planning-with-files"
      }
    },
    "local-resources": {
      "source": {
        "source": "directory",
        "path": ".claude/local/resources/linuxiscool/claude-resources"
      }
    }
  },
  "enabledPlugins": {
    "temporal@linuxiscool-claude-plugins-public": true,
    "koi@regen-ai": true,
    "ledger@regen-ai": true,
    "planning-with-files@planning-with-files": true,
    "claude-resources@claude-resources": true
  }
}
